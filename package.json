{
  "name": "sass-vars",
  "displayName": "sass-vars",
  "description": "Access your sass variables easily",
  "version": "0.0.1",
  "publisher": "tomesterez",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.showSassVariables"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.showSassVariables",
        "title": "Show SASS variables pannel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "@types/string_score": "^0.1.27",
    "color-diff": "^1.0.0",
    "color-string": "^1.2.0",
    "lodash": "^4.16.6",
    "nearest-color": "^0.4.0",
    "node-sass": "^3.11.1",
    "sass-to-js": "^1.3.0",
    "string_score": "^0.1.22"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.38",
    "@types/node-sass": "^3.10.30",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}
